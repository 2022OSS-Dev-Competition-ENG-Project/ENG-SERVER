<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.userservice.mapper.ImageMapper">


    <resultMap type="com.example.userservice.vo.GetProfileImageVo" id="getImageUrl">
        <result property="user_uuid" column="userUuid"/>
    </resultMap>

    <select id="getImageFile" resultMap="getImageUrl">
        select *
        from user
        where user_uuid = #{userUuid}
    </select>
</mapper>